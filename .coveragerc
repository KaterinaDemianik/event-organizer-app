[run]
source = .
omit = 
    */tests.py
    */test_*.py
    */*test*.py
    */migrations/*
    manage.py
    */venv/*
    */.venv/*
    */env/*
    */.env/*
    */settings_ci.py
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
